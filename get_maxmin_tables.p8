		imin=i
	end
	if (lmax!=max(#v,lmax)) then
		lmax=max(#v,lmax)
		imax=i
	end
end
print("min length:"..lmin)
print("max length:"..lmax)
print("min index:"..imin)
print("max index:"..imax)
