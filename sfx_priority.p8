pico-8 cartridge // http://www.pico-8.com
version 32
__lua__
function _init()
	cls()
	ppr=0
	s,c,p=-1,0,0
end

function _update()
	if btn(0) then
	s,c,p=0,0,0
	end
	if btn(1) then
	s,c,p=1,0,0
	end
	if btn(2) then
	s,c,p=2,0,0
	end
	if btn(3) then
	s,c,p=3,0,1
	end
	if btn(4) then
	s,c,p=4,0,2
	end
end

function _draw()
	cls()
	csfx(s,c,p)
	local msg="sfx priority"
	?msg,63-#msg*2,40,12
	?"now playing:"..stat(16),10,51,7
	?"choosed sound:"..s.."(priority:"..p..")",10,58,7
end

function csfx(s,c,p)
	--s:sfx num
	--c:channel
	--p:priority
	--ppr:priority of prev sfx(global)
	if s!=nil then
		local n=stat(16+c)
		if n<0 then
			sfx(s,c)
		else
			if ppr<p then
				sfx(s,c)
			end
		end
		ppr=p
	end
end
