function manga_effect()
	for d=0,1,rnd(.05)+.03 do
		local x,y,r=
		cos(d),sin(d),rnd(20)+30
		line(x*r+64,y*r+64,
		x*120+64,y*120+64,rnd({5,6}))
	end
end

